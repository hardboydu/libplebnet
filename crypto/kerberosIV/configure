  echo $ac_n "(cached) $ac_c" 1>&6
else
  if grep _version /etc/man.conf > /dev/null 2>&1; then
	krb_cv_sys_cat_suffix=0
else
	krb_cv_sys_cat_suffix=number
fi
fi

echo "$ac_t""$krb_cv_sys_cat_suffix" 1>&6
