PROG=	registerd
SRCS=	registerd.c
CFLAGS+=-DCRYPT -DKERBEROS -I${.CURDIR}/../../usr.bin/register \
	-I${.CURDIR}/../../include -Wall
DPADD+=	${LIBKDB} ${LIBKRB} ${LIBDES}
LDADD=	-lkdb -lkrb -ldes
MAN8=	registerd.8
BINDIR=	/usr/libexec

.include <bsd.prog.mk>
