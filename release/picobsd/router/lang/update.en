echo -n "Updating /etc contents on startup floppy...  "
mount /dev/fd0a /start_floppy
cd /etc
cp -Rp . /start_floppy/etc/
echo " Done."
echo -n "Updating kernel parameters...  "
kget -incore /start_floppy/kernel.config /stand/vanilla
umount /dev/fd0a
echo " Done."
