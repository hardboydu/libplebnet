DPADD=	${GDBLIBS} ${BULIBS} ${LIBKVM} ${LIBM} ${LIBREADLINE} ${LIBTERMCAP}
LDADD=	${GDBLIBS} ${BULIBS} -lkvm -lm -lreadline -ltermcap

