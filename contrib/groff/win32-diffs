+#  Makefile for Microsoft Visual C++ 6.0
+#  by Blake McBride  (blake@florida-software.com)
+
+
+.SUFFIXES : .cc
+
+.cc.obj:
+	cl -c -nologo -I../../include -Tp$<
+
+.c.obj:
+	cl -c -nologo -I../../include $<
+
+OBJS =	dvi.obj
+
+
+grodvi.exe : $(OBJS)
+	cl -nologo -Fe$@ *.obj ..\..\libs\libdriver\libdriver.lib ..\..\libs\libgroff\libgroff.lib
+
