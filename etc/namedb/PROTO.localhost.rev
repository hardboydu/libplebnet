@	IN	SOA	@host@. root.@host@.  (
				@date@	; Serial
				3600	; Refresh
				900	; Retry
				3600000	; Expire
				3600 )	; Minimum
	IN	NS	@host@.
1	IN	PTR	localhost.@domain@.
